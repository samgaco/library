{
  "parserOptions": { "ecmaVersion": 6 },
  "parser": "babel-eslint",
  "extends": "airbnb-base",
    rules: {
    'no-shadow': 0,
    'no-param-reassign': 0,
    'eol-last': 0,
  }
   rules: {
        "no-unused-vars": [
          "error",
          {
            varsIgnorePattern: "render"
          }
        ]
      }
}
